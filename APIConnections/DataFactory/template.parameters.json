{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "Credentials--AADTenantId": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/<your-subscription-id>/resourceGroups/<your-resource-group-name>/providers/Microsoft.KeyVault/vaults/<your-key-vault-name>"
        },
        "secretName": "Credentials--AADTenantId"
      }
    },
    "Credentials--AADClientId": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/<your-subscription-id>/resourceGroups/<your-resource-group-name>/providers/Microsoft.KeyVault/vaults/<your-key-vault-name>"
        },
        "secretName": "Credentials--AADClientId"
      }
    },
    "Credentials--AADClientSecret": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/<your-subscription-id>/resourceGroups/<your-resource-group-name>/providers/Microsoft.KeyVault/vaults/<your-key-vault-name>"
        },
        "secretName": "Credentials--AADClientSecret"
      }
    }
  }
}
